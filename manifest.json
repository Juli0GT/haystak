{
  "manifest_version": 2,

  "name": "Haysta:K",
  "description": "This extension helps developers efficiently find answers to FAQ's & PAQ's",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "*://*.google.com/*",
      "activeTab",
      "tabs",
      "https://ajax.googleapis.com/",
      "storage"
  ]
}
